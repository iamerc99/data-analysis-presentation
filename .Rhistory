# Load libraries
library(ggplot2)
library(readr)
# Read CSV
gdp_data <- read_csv("GDP.csv")  # replace with your file path
# Inspect
head(gdp_data)
# Plot
ggplot(gdp_data, aes(x = DATE, y = GDP)) +
geom_line(color = "steelblue") +
labs(title = "US GDP (Quarterly)", x = "Date", y = "Billions of Dollars") +
theme_minimal()
